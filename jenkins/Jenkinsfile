pipeline {

    agent any {

    environment {
    PATH = "C:\Program Files\Maven\apache-maven-3.9.2-bin:$PATH"
    }

            stages {
	    
        stage("Clone code") {

            steps {
                git credentialsId: 'git_credentials', url: https://github.com/christian-brothers-University/maven1.git

		}
	}

        stage("Build code") {

	steps {
	sh "mvn clean install"
            }
        }
        
}
